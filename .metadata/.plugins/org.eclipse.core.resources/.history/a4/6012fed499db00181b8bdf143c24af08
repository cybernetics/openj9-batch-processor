		this.isSack = isSack;
	}

	public short getIsChallenge() {
		return isChallenge;
	}

	public void setIsChallenge(short isChallenge) {
		this.isChallenge = isChallenge;
	}

	public short getIsChallengeReversed() {
		return isChallengeReversed;
	}

	public void setIsChallengeReversed(short isChallengeReversed) {
		this.isChallengeReversed = isChallengeReversed;
	}

	public String getChallenger() {
		return challenger;
	}

	public void setChallenger(String challenger) {
		this.challenger = challenger;
	}

	public short getIsMeasurement() {
		return isMeasurement;
	}

	public void setIsMeasurement(short isMeasurement) {
		this.isMeasurement = isMeasurement;
	}

	public short getIsIntercerption() {
		return isIntercerption;
	}

	public void setIsIntercerption(short isIntercerption) {
		this.isIntercerption = isIntercerption;
	}

	public short getIsFumble() {
		return isFumble;
	}

	public void setIsFumble(short isFumble) {
		this.isFumble = isFumble;
	}

	public short getIsPenalty() {
		return isPenalty;
	}

	public void setIsPenalty(short isPenalty) {
		this.isPenalty = isPenalty;
	}

	public short getIsTwoPointConversion() {
		return isTwoPointConversion;
	}

	public void setIsTwoPointConversion(short isTwoPointConversion) {
		this.isTwoPointConversion = isTwoPointConversion;
	}

	public short getIsTwoPointConversionSuccessful() {
		return isTwoPointConversionSuccessful;
	}

	public void setIsTwoPointConversionSuccessful(short isTwoPointConversionSuccessful) {
		this.isTwoPointConversionSuccessful = isTwoPointConversionSuccessful;
