	public short getYardLine() {
		return yardLine;
	}

	public void setYardLine(short yardLine) {
		this.yardLine = yardLine;
	}

	public short getSeriesFirstDown() {
		return seriesFirstDown;
	}

	public void setSeriesFirstDown(short seriesFirstDown) {
		this.seriesFirstDown = seriesFirstDown;
	}

	public short getNextScore() {
		return nextScore;
	}

	public void setNextScore(short nextScore) {
		this.nextScore = nextScore;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public short getTeamWin() {
		return teamWin;
	}

	public void setTeamWin(short teamWin) {
		this.teamWin = teamWin;
	}

	public short getSeasonYear() {
		return seasonYear;
	}

	public void setSeasonYear(short seasonYear) {
		this.seasonYear = seasonYear;
	}

	public short getYards() {
		return yards;
	}

	public void setYards(short yards) {
		this.yards = yards;
	}

	public String getFormation() {
		return formation;
	}

	public void setFormation(String formation) {
		this.formation = formation;
	}

	public String getPlayType() {
		return playType;
	}

	public void setPlayType(String playType) {
		this.playType = playType;
	}

	public short getIsRush() {
		return isRush;
	}

	public void setIsRush(short isRush) {
		this.isRush = isRush;
	}

	public short getIsPass() {
		return isPass;
	}

	public void setIsPass(short isPass) {
		this.isPass = isPass;
	}

	public short getIsIncomplete() {
		return isIncomplete;
	}

	public void setIsIncomplete(short isIncomplete) {
		this.isIncomplete = isIncomplete;
	}

	public short getIsTouchdown() {
		return isTouchdown;
	}

	public void setIsTouchdown(short isTouchdown) {
		this.isTouchdown = isTouchdown;
	}

	public String getPassType() {
		return passType;
