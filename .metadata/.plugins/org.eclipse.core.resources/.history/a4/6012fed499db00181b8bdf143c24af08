package com.ibm.developer.batchprocessor;

import java.util.Date;

public class FootballPlay {
	private long id;
	private long gameId;
	private Date gameDate;
	private short quarter;
	private short minute;
	private short second;
	private String offenseTeam;
	private String defenseTeam;
	private short down;
	private short toGo;
	private short yardLine;
	private short seriesFirstDown;
	private short nextScore;
	private String description;
	private short teamWin;
	private short seasonYear;
	private short yards;
	private String formation;
	private String playType;
	private short isRush;
	private short isPass;
	private short isIncomplete;
	private short isTouchdown;
	private String passType;
	private short isSack;
	private short isChallenge;
	private short isChallengeReversed;
	private String challenger;
	private short isMeasurement;
	private short isIntercerption;
	private short isFumble;
